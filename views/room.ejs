<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Chat APP</title>
    <link rel="stylesheet" href="/chatnew.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.3.1/socket.io.js"></script>
    <!-- <script src='http://localhost:5000/socket.io/socket.io.js'> </script> -->
    <script src='https://team-making.herokuapp.com/socket.io/socket.io.js'> </script>


</head>
<body class="showroom">
    
<div class="row">
    <div class="col-lg-2">
        <div class="online">
            <p class="users-online">Chat members:</p>
            <div class="users">
            </div>
        </div>
    </div>
    <div class="col-lg-10">
        <div class="chat-container">
            <div class="chat-header">
                <div class="logo">
                    <i class="fab fa-facebook-messenger"></i>
                    <h3>Messenger</h3>
                </div>
                <p ><a href="/">Back to home Page</a></p>
            </div>
    <!-- ------------------------------------ -->
            <div class="chat-section">
                <div class="main-wrapper">
                    <div class="chat-content">
                        <h1 class="room-message"></h1>
                        <div class="window">
                            <div class="chat-message">
                                <div id="output"></div>
                                <div id="feedback"></div>
                            </div>
                    </div>
        <!-- --------------------------------- -->
                        
                    </div>
    
            </div>
            <div>
                <h1 class="room-message"></h1>
    
                        <div class='fields'>
                            <input type="text" id="message" placeholder="Enter message">
                            <button id="send"><i class="fas fa-paper-plane"> Send</i></button>
                        </div>
            </div>
    
        </div>
    </div>
</div>
    <script src="/chat.js"></script>
</body>
</html>